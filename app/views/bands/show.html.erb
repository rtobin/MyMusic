<h1><%= @band.name %></h1>

<table>
  <tr>
    <th>Albums</th>
    <th>Time Added</th>
  </tr>
  <% @band.albums.each do |album| %>
    <tr>
      <td><li><%= link_to album.title, album_url(album.id) %></li></td>
      <td><%= album.created_at %></td>
    </tr>
  <% end %>
</table>
<br><br>
<%= link_to "back to bands", bands_url %>
<%= link_to "Edit Band", edit_band_url(@band) %>
